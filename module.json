
{
	"comment": "Module definition file. The name key 'name' should match the name of the module appended to 'appserver-', ex. appserver-directory",
	"name": "directory", 
	"description": "A module for testing the appserver functionality.",
	"files": ["Test.php"],
	"routes": {
				
         "test/committees":{
            "callback":"testDisplayCommitteeMembers",
            "content-type":"text/html",
            "access":"is_authenticated"
         },
         "committees": {
            "callback":"home",
            "content-type":"text\/html"
         },
         "get/committees":{
            "callback":"home2",
            "content-type":"application/json"
        },
        "directory/execute/%name":{
            "callback":"addCloudConvertJob",
            "content-type":"text\/html"
        },
        "directory/status/%jobId":{
         "callback":"getCloudConvertJobStatus",
         "content-type":"text\/html"
         },
        "directory/pdfs/%name":{
            "callback":"downloadPdf",
            "content-type":"application\/pdf"
        },
        "directory/savepdfs/%stuff/%name":{
            "callback":"saveCloudConvertPDF",
            "content-type":"text\/html"
        },
        "directory/webhook":{
         "callback":"jobWebhook",
         "content-type":"application\/json"
     }
    }
}

